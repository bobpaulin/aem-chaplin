<div class="row-fluid">
  <div class="span12">
    {{#each hits}}
        <div class="row-fluid comment">
            <div class="span12">
                <div class="row-fluid">
                    <div class="span6">{{this.comment}}</div>
                </div>
                <div class="row-fluid">
                  <div class="span12">User: {{this.userName}}</div>
                </div>
            </div>
         </div>
    {{/each}}
  </div>
</div>